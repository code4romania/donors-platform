        "postcss-import": "^12.0.1",
        "postcss-loader": "^3.0.0",
        "postcss-nested": "^4.2.3",
        "resolve-url-loader": "^3.1.0",
        "tailwindcss": "^1.5.2",
        "tailwindcss-aspect-ratio": "^3.0.0",
